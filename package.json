{
  "name": "weback-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for web pages using webpack",
  "main": "index.js",
  "keywords": [
    "webpack",
    "websites",
    "bootstrap",
    "jquery",
    "javascript"
  ],
  "author": "Daniel Stefanelli",
  "license": "ISC",
  "scripts": {
    "start:dev": "webpack-dev-server --config webpack.config.dev.js",
    "start": "webpack --config webpack.config.prod.js && http-server ./dist -o",
    "build": "webpack --config webpack.config.prod.js",
    "lint:js": "./node_modules/.bin/eslint src/js/*.js",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:html": "pa11y-ci ./src/**/*.html"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.7.4",
    "babel-loader": "^8.1.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "autoprefixer": "^9.1.5",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.20.2",
    "file-loader": "^5.0.2",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^3.0.0",
    "http-server": "^0.12.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^4.0.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.0.1",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^19.0.0"
  }
}
